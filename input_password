#include <stdio.h>

int main2(void)
{ 
	printf("what is your password? \n");
	short user_password = 0;
	scanf_s("%hd", &user_password);

	if (user_password == 9999)
	{
		printf("you remembered your password \n");
	}
	else (user_password != 9999);
	{
		printf("you need to write down your password and remember it");
	}

	return 0;
}
